{
    "manifest_version": 2,
    "name": "Extens√£o de Testes",
    "version": "0.0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_popup": "index.html"
      }
}